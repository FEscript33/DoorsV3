local ConTab = Window:CreateTab("Config", 109523353085474)

local Label = FETab:CreateLabel("FTU X " .. game.Players.LocalPlayer.Name, "rewind")

local Button = ConTab:CreateButton({
   Name = "Unload",
   Callback = function()
   Rayfield:Destroy()
   end,
})
